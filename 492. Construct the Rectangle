class Solution {
public:
    vector<int> constructRectangle(int area) {
        vector<vector<int>> ans;
        for(int i=1;i*i<=area;i++) {
            vector<int>temp;
            //store all factors pairs
            if(area%i==0) {
                temp.push_back(i);
                temp.push_back(area/i);
                sort(temp.rbegin(),temp.rend());
                ans.push_back(temp);
            }
        }
        

      vector<int> res(2,0);
     int min = 1e9;   
    for(int i=0;i<ans.size();i++) {
    //find factor pair with minimum difference
       int diff= ans[i][0] - ans[i][1];
        if(diff<min) {
            min=diff;
            res[0]=ans[i][0];
            res[1]=ans[i][1];
        }
    }
        return res;
    }
};
