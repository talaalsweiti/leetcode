class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int window = 0 ;
        int max = -1e9;
        vector<int> sum;
        for(int i=0;i<nums.size();i++) {
            int l =0;
            int r = window ;
            int temp =0;  
            while(l<=window) {
                temp+=nums[l];
                l++;
            }    
            l=0;
            if(temp>max) max =temp;
            for(int j=window+1;j<nums.size();j++) {
                temp-=nums[l];
                temp+=nums[j];
                if(temp>max) max=temp;
                l++;
            }
            window++;
        }
	
            return max;
    }
};
