class Solution {
public:
    int maximum69Number (int num) {
        int temp = num , cnt =0 ;
        string str=  to_string(num); ;
        for(int i=0;i<str.size();i++) {
            if(str[i]=='6'){
             int k = str.size() - i;
               if(k==4) num+=3000;
               if(k==3) num+=300;
               if(k==2) num+=30;
               if(k==1) num+=3; 
        break;
            }
        }
        return num;
     
    }
};
