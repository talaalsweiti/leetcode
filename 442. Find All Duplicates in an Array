class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        vector<int> ans;
        map<int,int> mp;
        int n = nums.size();
        for(int i=0;i<n;i++) {
          mp[nums[i]]++;
          if( mp[nums[i]]==2 ) {
              ans.push_back(nums[i]);  
              mp[nums[i]] =0 ;
          } 
        }
        return ans;
    }
};
